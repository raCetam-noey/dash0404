<mvc:View
	controllerName="dash0404.controller.AnalyzeProgram"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.uxap"
	xmlns:m="sap.m"
	xmlns:c="sap.ui.core"
	xmlns:dnd="sap.ui.core.dnd"
	xmlns:w="sap.ui.integration.widgets"
	xmlns:l="sap.ui.layout"
	xmlns:form="sap.ui.layout.form"
	xmlns:f="sap.f"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
>
	<ObjectPageLayout
		id="ObjectPageLayout"
		headerContentPinnable="false"
		showAnchorBar="false"
	>
		<headerTitle>
			<ObjectPageHeader
				id="ObjectPageHeader"
				objectTitle="Analyze Program"
				class="sapUiContentPadding"
			/>
		</headerTitle>
		<sections>
			<ObjectPageSection
				id="_IDGenObjectPageSection1"
				titleUppercase="false"
				title="Connection Status"
			>
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<m:MessageStrip
								text="{i18n>MessageStripText}
									{i18n>MessageStripTextSub1}
									{i18n>MessageStripTextSub2}
									{i18n>MessageStripText2}
									{i18n>MessageStripText3}"
								showIcon="true"
								class="sapUiContentPadding"
							/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
				<subSections>
					<ObjectPageSubSection
						id="_IDGenObjectPageSubSection1"
						title="프로그램 사용현황 분석"
						showTitle="false"
					>
						<blocks>
							<m:Table
								id="analyzeTable"
								class="sapUxAPObjectPageSubSectionAlignContent"
								width="auto"
								items="{/}"
								fixedLayout="false"
							>
								<m:headerToolbar>
									<m:OverflowToolbar id="_IDGenOverflowToolbar1">
										<m:Title
											id="_IDGenTitle1"
											text="Usage Result"
											level="H2"
										/>
										<m:ToolbarSpacer id="_IDGenToolbarSpacer1"/>
										<m:SearchField
											id="_IDGenSearchField1"
											width="17.5rem"
										/>
										<m:OverflowToolbarButton
											id="_IDGenOverflowToolbarButton1"
											tooltip="Sort"
											text="Sort"
											icon="sap-icon://sort"
										/>
										<m:OverflowToolbarButton
											id="_IDGenOverflowToolbarButton2"
											tooltip="Filter"
											text="Filter"
											icon="sap-icon://filter"
										/>
										<m:OverflowToolbarButton
											id="_IDGenOverflowToolbarButton3"
											tooltip="Group"
											text="Group"
											icon="sap-icon://group-2"
										/>
										<m:OverflowToolbarButton
											id="_IDGenOverflowToolbarButton4"
											tooltip="Settings"
											text="Settings"
											icon="sap-icon://action-settings"
										/>
									</m:OverflowToolbar>
								</m:headerToolbar>
								<m:columns>
									<m:Column id="_IDGenColumn1">
										<m:Text
											id="_Text6"
											text="TCODE"
										/>
									</m:Column>
									<m:Column id="_IDGenColumn2">
										<m:Text
											id="_Text7"
											text="Transaction 명"
										/>
									</m:Column>
									<m:Column id="_IDGenColumn3">
										<m:Text
											id="_Text8"
											text="1월"
										/>
									</m:Column>
									<m:Column id="_IDGenColumn4">
										<m:Text
											id="_Text9"
											text="2월"
										/>
									</m:Column>
									<m:Column id="_IDGenColumn5">
										<m:Text
											id="_Text10"
											text="3월"
										/>
									</m:Column>
									<m:Column>
										<m:Text
											id="_Text11"
											text="4월"
										/>
									</m:Column>
									<m:Column>
										<m:Text
											id="_Text12"
											text="5월"
										/>
									</m:Column>
									<m:Column>
										<m:Text
											id="_Text13"
											text="6월"
										/>
									</m:Column>
									<m:Column>
										<m:Text
											id="_Text14"
											text="7월"
										/>
									</m:Column>
									<m:Column>
										<m:Text
											id="_Text15"
											text="8월"
										/>
									</m:Column>
									<m:Column>
										<m:Text
											id="_Text16"
											text="9월"
										/>
									</m:Column>
									<m:Column>
										<m:Text
											id="_Text17"
											text="10월"
										/>
									</m:Column>
									<m:Column>
										<m:Text
											id="_Text18"
											text="11월"
										/>
									</m:Column>
									<m:Column>
										<m:Text
											id="_Text19"
											text="12월"
										/>
									</m:Column>
									<m:Column>
										<m:Text
											id="_Text20"
											text="계"
										/>
									</m:Column>
								</m:columns>
								<m:items>
									<m:ColumnListItem
										id="_IDGenColumnListItem1"
										vAlign="Middle"
									>
										<!-- <m:customData>
											<c:CustomData key="myData" value="{Tcode}" writeToDom="true"/>
										</m:customData> -->
										<m:cells>
											<m:Title
												textAlign="Center"
												id="_Text4"
												text="{Tcode}"
												wrapping="false"
											/>
											<m:Text
												id="_Text5"
												text="{Ttext}"
												wrapping="false"
											/>
											<m:Text
												id="1"
												text="{Use01}"
												wrapping="false"
											/>
											<m:Text
												id="2"
												text="{Use02}"
												wrapping="false"
											/>
											<m:Text
												id="3"
												text="{Use03}"
												wrapping="false"
											/>
											<m:Text
												id="4"
												text="{Use04}"
												wrapping="false"
											/>
											<m:Text
												id="5"
												text="{Use05}"
												wrapping="false"
											/>
											<m:Text
												id="6"
												text="{Use06}"
												wrapping="false"
											/>
											<m:Text
												id="7"
												text="{Use07}"
												wrapping="false"
											/>
											<m:Text
												id="8"
												text="{Use08}"
												wrapping="false"
											/>
											<m:Text
												id="9"
												text="{Use09}"
												wrapping="false"
											/>
											<m:Text
												id="10"
												text="{Use10}"
												wrapping="false"
											/>
											<m:Text
												id="11"
												text="{Use11}"
												wrapping="false"
											/>
											<m:Text
												id="12"
												text="{Use12}"
												wrapping="false"
											/>
											<m:ObjectStatus
												class="style"
												id="_ObjectStatus1"
												text="{Total}"
												state="Success"
											/>
										</m:cells>
									</m:ColumnListItem>
								</m:items>
							</m:Table>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
</mvc:View>